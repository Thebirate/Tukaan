@model List<Category>


    <div class="container">
        <div class="row pt-4">
            <div class="col-6">

                <h2 class="text-primary">
                    Category List
                </h2>
            </div>
        <div class="col-6">

            <h2 class="text-primary">
                Display Order
                </h2>
            </div>

        </div>

<table class="table table-bordered table-striped">
    <thead>
        
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder)){
        <tr>
            <td>@obj.Name </td>
            <td>  @obj.DisplayOrder</td>
        </tr>
        }
    </tbody>
</table>


</div>